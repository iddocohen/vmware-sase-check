{
  "manifest_version": 2,

  "name": "VMware SASE Browser Checker",
  "description": "Checking browser setting to determine if browser can reach Cloud Web Security (CWS) functionalities",
  "version": "0.1",
  "author": "Iddo Cohen",

  "icons": { 
        "16": "icon/icon16-white.png",
        "48": "icon/icon48-white.png",
        "128": "icon/icon128-white.png" 
  },
  "browser_action": {
    "default_icon": "icon/icon16-white.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["js/vmchecker.background.js"],
   "persistent": false
  }
}
