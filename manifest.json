{
  "manifest_version": 2,

  "name": "VMware SASE Browser Checker",
  "description": "Checking browser setting to determine if browser can reach Cloud Web Security (CWS) functionalities",
  "version": "0.1",

  "icons": { 
        "16": "icon/icon16-white.png",
        "48": "icon/icon48-white.png",
        "128": "icon/icon128-white.png" 
  },
  "browser_action": {
    "default_icon": "icon/icon16-white.png",
    "default_popup": "vmchecker.popup.html"
  },
  
  "permissions": [
    "proxy",
    "https://safe-cws-sase.vmware.com/*",
    "*://www.vmware.com/*",
    "https://*.google.com/*",
    "https://*.youtube.com/*",
    "https://*.facebook.com/*",
    "https://*.twitter.com/*",
    "https://*.instagram.com/*",
    "https://*.stackoverflow.com/*",
    "https://*.wikipedia.org/*",
    "https://*.live.com/*",
    "https://*.yahoo.com/*",
    "https://*.amazon.com/*"
  ],
  "background": {
        "scripts": ["js/vmchecker.background.js"]
  }
}
