{
  "manifest_version": 2,
  "name": "VMware CWS Checker",
  "description": "Auto test performance and security enforcements against VMware Cloud Web Security (CWS) offering.",
  "version": "0.21",
  "author": "Iddo Cohen",
  "incognito":"split",
  "icons": { 
        "16": "icon/icon16-white.png",
        "48": "icon/icon48-white.png",
        "128": "icon/icon128-white.png" 
  },
  "browser_action": {
    "default_icon": "icon/icon16-white.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["js/vmchecker.background.js"],
   "persistent": false
  }
}
