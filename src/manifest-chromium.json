{
  "manifest_version": 3,
  "name": "VMware CWS Checker",
  "description": "Auto test performance and security enforcement against VMware Cloud Web Security (CWS) offering.",
  "version": "0.22",
  "author": "Iddo Cohen",
  "incognito":"split",
  "icons": { 
        "16": "icon/icon16-white.png",
        "48": "icon/icon48-white.png",
        "128": "icon/icon128-white.png" 
  },
  "action": {
    "default_icon": "icon/icon16-white.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "js/vmchecker.background.js",
    "type": "module"
  },
  "minimum_chrome_version": "92"
}
